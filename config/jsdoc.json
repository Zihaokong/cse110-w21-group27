{
    "source": {
        "include": ["source"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|admin|images|specs)"
    },
    "plugins" : ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs"
    }
}
