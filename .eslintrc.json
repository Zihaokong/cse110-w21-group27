{
    "extends": ["airbnb-base", "prettier", "plugin:cypress/recommended"],
    "plugins": ["prettier", "jest"],
    "ignorePatterns": ["/build/*"],
    "rules": {
        "no-unused-vars":"warn",
        "no-plusplus":"off",
        "no-use-before-define":"off",    

        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto",
              "singleQuote": true
            }
          ]
    },
    "env": {
        "jest/globals": true,
        "browser": true
    }
}